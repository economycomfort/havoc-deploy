[
    {
        "name": "${campaign_id}-metasploit",
        "image": "public.ecr.aws/havoc_sh/metasploit:latest",
        "essential": true,
        "entryPoint": [
            "/bin/bash", "-c"
        ],
        "command": [
            "/usr/bin/supervisord", "-c", "/etc/supervisor/conf.d/supervisord.conf"
        ],
        "logConfiguration": {
            "logDriver": "awslogs",
            "options": {
                "awslogs-group": "${campaign_id}/ecs",
                "awslogs-region": "${aws_region}",
                "awslogs-stream-prefix": "metasploit"
            }
        }
    }
]